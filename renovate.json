{
  "extends": [
    "config:base",
    ":pinDependencies"
  ],
  "prConcurrentLimit": 3,
  "packageRules": [
    {
      "packageRules": [
        {
          "matchUpdateTypes": [
            "patch"
          ],
          "groupName": "all patch dependencies",
          "groupSlug": "all-patch",
          "automerge": true
        }
      ]
    },
    {
      "matchUpdateTypes": [
        "pin",
        "minor",
        "patch"
      ],
      "automerge": true
    }
  ],
  "npmrc": "@mercell:registry=https://npm.pkg.github.com/",
  "hostRules": [
    {
      "matchHost": "https://npm.pkg.github.com/",
      "hostType": "npm",
      "encrypted": {
        "token": ""
      }
    },
    {
      "matchHost": "https://rubygems.pkg.github.com/mercell",
      "hostType": "rubygems",
      "encrypted": {
        "token": ""
      }
    }
  ]
}